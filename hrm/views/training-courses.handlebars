<div class="container mt-5 pt-5">
  <h1>Courses Selected</h1>
  <table class="table mt-5">
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Trainer</th>
      <th>Coordinator</th>
      <th>Payment Type</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <td>{{code}}</td>
      <td>{{name}}</td>
      <td>{{trainer}}</td>
      <td>{{coordinator}}</td>
      <td>{{paymentType}}</td>
      <td >
        <a href="/training-sessions/{{employeeId}}/{{name}}" class="btn btn-success"><i class="fas fa-user-graduate"></i></a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>

</div>